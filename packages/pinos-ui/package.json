{
  "name": "pinos-ui",
  "version": "0.1.0-alpha.5",
  "description": "A Vue 3 UI library",
  "author": "eric<https://github.com/GOGOGOSIR>",
  "license": "MIT",
  "homepage": "https://pinos-ui.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinosJs/pinos-ui.git"
  },
  "bugs": {
    "url": "https://github.com/pinosJs/pinos-ui/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/lib/index.cjs",
      "import": "./dist/es/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/lib/index.cjs",
  "module": "./dist/es/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "styles",
    "types.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "esno scripts/build.ts",
    "build:style": "gulp --require sucrase/register/ts"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@pinos-ui/utils": "workspace:^"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.3",
    "@types/fs-extra": "9.0.13",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-sass": "^5.0.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "consola": "^2.15.3",
    "esbuild": "^0.15.13",
    "eslint": "^8.36.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "10.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "rollup": "2.79.1",
    "sass": "^1.56.1",
    "sucrase": "^3.28.0",
    "typescript": "^4.8.3",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.1",
    "vue": "3.2.40"
  }
}
